    <!-- http://www.semanticweb.org/irina/ontologies/2024/0/bogx#Aethionema_grandiflorum -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/irina/ontologies/2024/0/bogx#Aethionema_grandiflorum">
        <rdf:type rdf:resource="http://www.semanticweb.org/irina/ontologies/2024/0/bogx#Plant"/>
