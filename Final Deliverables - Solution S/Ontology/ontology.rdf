        <j.0:plantId>urn:uuid:abcd6083-12af-45e7-9d04-29e9eddfd3bd</j.0:plantId>
        <j.0:plantImageURL>http://commons.wikimedia.org/wiki/Special:FilePath/Malephoracrocea.jpg?width=300</j.0:plantImageURL>
